{
  "name": "vsnotebook",
  "displayName": "vsnotebook",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:notebook-view"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "notebook-view-container",
          "title": "Notebook View Container",
          "icon": "assets/img/notebook-view-container.svg"
        }
      ]
    },
    "views": {
      "notebook-view-container": [
        {
          "id": "notebook-view",
          "name": "Notebook View"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile:prod",
    "compile:prod": "webpack --mode production",
    "compile:dev": "webpack --mode development",
    "lint": "eslint src --ext ts",
    "watch": "python3 main.py && webpack --mode development --watch",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@jupyterlab/services": "^4.2.0",
    "axios": "^0.19.2"
  }
}
